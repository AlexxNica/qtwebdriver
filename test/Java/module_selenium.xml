      <classpath refid="selenium.module.production.classpath"/>
      <src refid="selenium.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.selenium"/>
    </javac>
    
    <copy todir="${selenium.output.dir}">
      <fileset dir="${module.selenium.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.selenium.tests" depends="compile.module.selenium.production" description="compile module Selenium; test classes" unless="skip.tests">
