    <mkdir dir="${selenium.output.dir}"/>
    <javac destdir="${selenium.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.selenium}/javac">
